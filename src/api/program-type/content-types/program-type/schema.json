{
  "kind": "collectionType",
  "collectionName": "program_types",
  "info": {
    "singularName": "program-type",
    "pluralName": "program-types",
    "displayName": "Program Types",
    "description": "Tipos de programas disponibles"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "icon": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "destinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "inversedBy": "programTypes"
    }
  }
}
