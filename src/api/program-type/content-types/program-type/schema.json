{
  "kind": "collectionType",
  "collectionName": "program_types",
  "info": {
    "singularName": "program-type",
    "pluralName": "program-types",
    "displayName": "Program Types",
    "description": "Tipos de programas disponibles"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "description": {
      "type": "text"
    },
    "requirements": {
      "type": "component",
      "repeatable": true,
      "component": "requirement.requirement"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "programs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::program.program",
      "mappedBy": "programType"
    }
  }
}
