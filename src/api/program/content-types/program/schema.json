{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Programs",
    "description": "Programas de estudio y trabajo en el extranjero"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "shortDescription": {
      "type": "text",
      "maxLength": 500
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "location": {
      "type": "string",
      "required": true
    },
    "mostrarDestinos": {
      "type": "boolean",
      "default": false
    },
    "destinationsToShow": {
      "type": "json"
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "startDates": {
      "type": "string"
    },
    "ageRequirement": {
      "type": "json"
    },
    "ageRange": {
      "type": "json"
    },
    "englishLevel": {
      "type": "string"
    },
    "programType": {
      "type": "enumeration",
      "enum": [
        "work-and-travel",
        "estudios-exterior",
        "practicas-pasantias",
        "au-pair",
        "voluntariados"
      ],
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "string",
      "default": "USD"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "destination": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::destination.destination"
    },
    "features": {
      "type": "json"
    },
    "requirements": {
      "type": "json",
      "required": true
    },
    "simple_requirements": {
      "type": "json"
    },
    "beneficios": {
      "type": "json",
      "required": true
    },
    "inclusions": {
      "type": "json"
    },
    "noincluye": {
      "type": "json"
    },
    "tipos_actividades": {
      "type": "component",
      "repeatable": true,
      "component": "program.activity-type"
    },
    "support": {
      "type": "json"
    },
    "certification": {
      "type": "json"
    },
    "languageRequirements": {
      "type": "json"
    },
    "testimonials": {
      "type": "component",
      "repeatable": true,
      "component": "program.testimonial"
    },
    "faq": {
      "type": "json"
    },
    "pricing": {
      "type": "json"
    },
    "oportunidades_laborales": {
      "type": "json"
    },
    "visa": {
      "type": "json",
      "required": true
    },
    "why_program": {
      "type": "json"
    },
    "why_stc": {
      "type": "json",
      "required": true
    },
    "carreras_afines": {
      "type": "json"
    },
    "vacantes_disponibles": {
      "type": "json"
    },
    "tipos_programas": {
      "type": "json"
    },
    "dato_curioso": {
      "type": "text"
    },
    "como_ayuda_agencia": {
      "type": "component",
      "repeatable": false,
      "component": "program.agency-help",
      "required": true
    },
    "planificacion": {
      "type": "json",
      "required": true
    },
    "solvencia_economica": {
      "type": "text",
      "required": true
    },
    "casos_exito": {
      "type": "json"
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "text"
    },
    "seoKeywords": {
      "type": "json"
    }
  }
}