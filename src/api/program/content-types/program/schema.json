{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Programs",
    "description": "Programas de estudio y trabajo en el extranjero"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "programType": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::program-type.program-type",
      "inversedBy": "programs"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::destination.destination"
    },
    "duration": {
      "type": "component",
      "repeatable": false,
      "component": "duration.duration"
    },
    "startDates": {
      "type": "json"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "features": {
      "type": "json"
    },
    "requirements": {
      "type": "json"
    },
    "como_ayuda_agencia": {
      "type": "json"
    },
    "beneficios": {
      "type": "json"
    },
    "visa": {
      "type": "json"
    },
    "planificacion": {
      "type": "json"
    },
    "solvencia_economica": {
      "type": "json"
    }
  }
}