{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Programs",
    "description": "Programas de estudio y trabajo en el extranjero"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "programType": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::program-type.program-type",
      "inversedBy": "programs"
    },
    "destinos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "inversedBy": "programs"
    },
    "duration": {
      "type": "string"
    },
    "startDates": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "program-features.feature"
    },
    "como_ayuda_agencia": {
      "type": "component",
      "repeatable": false,
      "component": "como-ayuda-agencia.como-ayuda-agencia"
    },
    "beneficios": {
      "type": "dynamiczone",
      "components": [
        "benefits.program",
        "benefits.pais",
        "benefits.lugares-iconicos",
        "benefits.razones-experiencia"
      ]
    },
    "visa": {
      "type": "component",
      "repeatable": false,
      "component": "program-visa.program-visa"
    },
    "planificacion": {
      "type": "component",
      "repeatable": false,
      "component": "program-planificacion.program-planificacion"
    },
    "solvencia_economica": {
      "type": "string"
    },
    "requirements": {
      "type": "component",
      "repeatable": false,
      "component": "program-requirements.program-requirements"
    },
    "why_program": {
      "type": "component",
      "repeatable": true,
      "component": "why-program.why-program"
    },
    "simple_requirements": {
      "type": "component",
      "repeatable": true,
      "component": "simple-requirements.simple-requirements"
    },
    "why_stc": {
      "type": "component",
      "repeatable": true,
      "component": "why-stc.why-stc"
    },
    "casos_exito": {
      "type": "component",
      "repeatable": true,
      "component": "casos-exito.casos-exito"
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "faq.faq"
    },
    "tipos_programas": {
      "type": "component",
      "repeatable": false,
      "component": "tipos-programas.tipos-programas"
    },
    "oportunidades_laborales": {
      "type": "component",
      "repeatable": false,
      "component": "oportunidades-laborales.oportunidades-laborales"
    },
    "tipos_actividades": {
      "type": "component",
      "repeatable": true,
      "component": "tipos-actividades.tipos-actividades"
    },
    "vacantes_disponibles": {
      "type": "json"
    },
    "carreras_afines": {
      "type": "json"
    },
    "testimonials": {
      "type": "component",
      "repeatable": true,
      "component": "testimonials.testimonials"
    },
    "noincluye": {
      "type": "json"
    }
  }
}